{
  "name": "fc-transform",
  "version": "0.0.15",
  "description": "This is a component demo for Serverless Devs Tool ",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "autoInstall": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "prebuild": "rm -rf node_modules && rm -rf package-lock.json && npm i && rimraf lib",
    "build": "ncc build src/index.ts -m -e @serverless-devs/core -o lib",
    "doc": "npx typedoc src/index.ts --json doc/doc.json --out doc"
  },
  "dependencies": {
    "@serverless-devs/core": "^0.0.*",
    "fs-extra": "^10.0.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.7",
    "@vercel/ncc": "^0.24.0"
  }
}
